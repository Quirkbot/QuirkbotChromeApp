{
	"name": "Quirkbot for Chrome",
	"version": "0.0.1",
	"description": "Allows you to program Quirkbots directly from your browser.",
	"default_locale": "en",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"app": {
		"background": {
			"scripts": [
				"src/libs/chrome-arduino.js",
				"src/libs/ChromeExternalAPIServer.js",
				"src/libs/v86/libv86.js",
				"src/libs/v86/compiler.js",
				"src/Utils.js",
				"src/QUIRKBOT_RECOVERY_SKETCH.js",
				"src/serial.js",
				"src/HexUploader.js",
				"src/Quirkbot.js",
				"src/main.js"
			]
		}
	},
	"externally_connectable": {
		"matches": [
			"*://quirkbot.com/*",
			"*://quirkbot.dev/*",
			"*://quirkbot.herokuapp.com/*"
		]
	},
	"permissions": [
		"serial",
		"usb"
	],
	"key": ""
}
